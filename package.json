{
  "name": "astro-cactus",
  "version": "6.1.1",
  "private": false,
  "packageManager": "bun@1.2.7",
  "scripts": {
    "dev": "tinacms dev -c \"astro dev\"",
    "start": "astro dev",
    "build": "astro build",
    "postbuild": "pagefind --site dist",
    "preview": "astro preview",
    "lint": "biome lint .",
    "format": "bun run format:code && bun run format:imports",
    "format:code": "biome format . --write && prettier -w \"**/*\" \"!**/*.{md,mdx}\" --ignore-unknown --cache",
    "format:imports": "biome check --formatter-enabled=false --write",
    "check": "astro check"
  },
  "dependencies": {
    "@astrojs/markdown-remark": "6.2.0",
    "@astrojs/mdx": "4.1.0",
    "@astrojs/rss": "4.0.11",
    "@astrojs/sitemap": "3.3.0",
    "@iconify-json/simple-icons": "^1.2.29",
    "@playform/compress": "^0.1.8",
    "@tailwindcss/vite": "4.0.17",
    "astro": "5.5.5",
    "astro-expressive-code": "^0.40.2",
    "astro-icon": "^1.1.5",
    "astro-robots-txt": "^1.0.0",
    "astro-webmanifest": "^1.0.0",
    "cssnano": "^7.0.6",
    "hastscript": "^9.0.1",
    "mdast-util-directive": "^3.1.0",
    "mdast-util-to-markdown": "^2.1.2",
    "mdast-util-to-string": "^4.0.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-external-links": "^3.0.0",
    "rehype-unwrap-images": "^1.0.0",
    "remark-directive": "^3.0.1",
    "satori": "0.12.1",
    "satori-html": "^0.3.2",
    "tinacms": "^2.7.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@biomejs/biome": "^1.9.4",
    "@iconify-json/mdi": "^1.2.3",
    "@pagefind/default-ui": "^1.3.0",
    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tinacms/cli": "^1.9.3",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.13.14",
    "autoprefixer": "^10.4.21",
    "pagefind": "^1.3.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "reading-time": "^1.5.0",
    "tailwindcss": "4.0.0",
    "typescript": "^5.8.2"
  }
}
